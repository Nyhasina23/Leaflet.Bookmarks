{
    "name": "Leaflet.Bookmarks",
    "version": "0.0.1",
    "description": "Leaflet plugin for user-generated bookmarks",
    "main": "dist/Leaflet.Bookmarks.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "bower install && npm run watch & serve",
        "watch": "watchify -v -d -u leaflet examples/app.js -o examples/bundle.js",
        "build": "browserify -u leaflet index.js -o dist/Leaflet.Bookmarks.js",
        "compress": "browserify -u leaflet index.js | uglifyjs -m -c drop_console=true,warnings=false > dist/Leaflet.Bookmarks.min.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/w8r/Leaflet.Bookmarks"
    },
    "keywords": [
        "leaflet",
        "bookmarks",
        "plugin"
    ],
    "author": "Alexander Milevski",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/w8r/Leaflet.Bookmarks/issues"
    },
    "homepage": "https://github.com/w8r/Leaflet.Bookmarks",
    "dependencies": {
        "Leaflet.contextmenu": "git+https://github.com/aratcliffe/Leaflet.contextmenu#e7c871822edfa602e21128338c97f56401a89e25"
    },
    "browserify": {
        "transform": [
            "deamdify"
        ]
    },
    "devDependencies": {
        "bower": "^1.3.12",
        "browserify": "^6.0.2",
        "deamdify": "^0.1.1",
        "debowerify": "^0.8.2",
        "exorcist": "^0.1.6",
        "grunt": "^0.4.5",
        "grunt-concurrent": "^1.0.0",
        "grunt-contrib-concat": "^0.5.0",
        "grunt-contrib-connect": "^0.8.0",
        "grunt-contrib-copy": "^0.5.0",
        "grunt-contrib-less": "^0.11.4",
        "grunt-contrib-uglify": "^0.5.1",
        "grunt-contrib-watch": "^0.6.1",
        "leaflet": "^0.7.3",
        "serve": "^1.4.0",
        "uglify-js": "^2.4.15",
        "watchify": "^1.0.6"
    }
}
